//package
package Main;
//imports
import monster.MON_IceClops;
import monster.MON_VaseOfDeath;
//public class AssetSetter
public class AssetSetter{
	//GamePanel gp
	GamePanel gp;
	//public AssetSetter constructor
	public AssetSetter(GamePanel gp) {
		//this.gp=gp;
		this.gp=gp;
	}//end AssetSetter constructor
	//public void setMonster
	public void setMonster() {
	gp.monster[0]= new MON_IceClops(gp);
	gp.monster[0].worldX=gp.tileSize*7;
	gp.monster[0].worldY=gp.tileSize*6;
	
	gp.monster[1]=new MON_VaseOfDeath(gp);
	gp.monster[1].worldX=gp.tileSize*3;
	gp.monster[1].worldY=gp.tileSize*4;
	}//end public void setMonster method
}//end public class AssetSetter
